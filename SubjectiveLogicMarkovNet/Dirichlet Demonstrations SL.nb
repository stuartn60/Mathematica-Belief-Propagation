 RowBox[{
  RowBox[{
   RowBox[{
   "Print", "[", 
    "\"\<Is the result of the ProductDistribution of two \
DirichletDistributions approximately a DirichletDistribution? ... Answer: It \
may be.\>\"", "]"}], ";"}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"SeedRandom", "[", "9871", "]"}], ";"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Alpha]Array", "=", 
   RowBox[{"MapIndexed", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Indexed", "[", 
         RowBox[{"\[Alpha]", ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"First", "@", "#2"}], ",", "i"}], "}"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", 
          RowBox[{"1", "+", 
           RowBox[{"Times", "@@", "#1"}]}]}], "}"}]}], "]"}], "&"}], ",", 
     RowBox[{"fposI1", "[", 
      RowBox[{"[", 
       RowBox[{"All", ",", "2"}], "]"}], "]"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"beliefs", "=", 
   RowBox[{"fposI1", "[", 
    RowBox[{"[", 
     RowBox[{"All", ",", "5"}], "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Alpha]s", "=", 
   RowBox[{"Quiet", "@", 
    RowBox[{"Solve", "[", 
     RowBox[{
      RowBox[{"Flatten", "@", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"Thread", "[", 
            RowBox[{
             RowBox[{"Mean", "@", 
              RowBox[{"DirichletDistribution", "[", 
               RowBox[{"\[Alpha]Array", "[", 
                RowBox[{"[", "#", "]"}], "]"}], "]"}]}], "\[Equal]", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"1", "/", 
                RowBox[{"Length", "@", "beliefs"}]}], ")"}], 
              RowBox[{"beliefs", "[", 
               RowBox[{"[", "#", "]"}], "]"}]}]}], "]"}], "&"}], "/@", 
          RowBox[{"Range", "@", 
           RowBox[{"Length", "@", "beliefs"}]}]}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"Thread", "[", 
              RowBox[{
               RowBox[{"Rest", "@", "#"}], ">", "0"}], "]"}], ",", 
             RowBox[{
              RowBox[{"First", "@", "#"}], "\[Equal]", "0.1"}]}], "}"}], 
           "&"}], "/@", "\[Alpha]Array"}]}], "}"}]}], ",", 
      RowBox[{"Flatten", "@", "\[Alpha]Array"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Alpha]\[ScriptCapitalD]", "=", 
